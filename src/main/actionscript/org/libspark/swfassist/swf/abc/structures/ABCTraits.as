 function set init( value:ABCMethodInfo ):void 		{ 			_init = value; 						if( _init != null )				_init.kind = ABCConsts.TRAIT_Function;		};				public function get traits():Array		{			if ( _traits==null ) _traits = new Array(); 				return _traits; 		}		public function ABCTraits():void		{			_traits = null;		}			}}