blic var debugName      	:String;		public var flags		  	:uint;		public var body 		  	:ABCMethodBody; // ABC opcodes for this method		public var activation		:ABCTraits; // traits 				/* annonimouse function, created somewhere in code */		public var annonimous:Boolean = false;				public function ABCMethodInfo()		{			returnType = null;			debugName = null;			paramTypes = null;			optionalValues = null;			paramNames = null;			body = null;			annonimous = true;		}	}		}